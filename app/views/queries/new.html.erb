<h1>Create Your Awesome Query</h1>

<%= form_for @query , url: queries_path do |f| %>
	<p>
		<%= f.label :question %><br/>
		<%= f.text_field :question %><br/>
	</p>

	<p>
		<%= f.label :description %><br/>
		<%= f.text_area :description %><br/>
	</p>

	<p>
		<%= f.label :topic %><br/>
		<%= f.text_area :topic %><br/>
	</p>


	<p>
		<%= f.submit %>
	</p>

	<% end %>
